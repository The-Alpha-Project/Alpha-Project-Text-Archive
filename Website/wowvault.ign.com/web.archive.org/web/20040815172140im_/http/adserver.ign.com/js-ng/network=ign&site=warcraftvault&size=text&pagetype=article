N2) { // NN2 cannot handle Dates, so skip this part");document.writeln("	    if ( (typeof(hours) == 'string') && Date.parse(hours) ) { // already a Date string");document.writeln("		var numHours = hours;");document.writeln("	    } else if (typeof(hours) == 'number') { // calculate Date from number of hours");document.writeln("		var numHours = (new Date((new Date()).getTime() + hours*3600000)).toGMTString();");document.writeln("	    }");document.writeln("	}");document.writeln("	document.cookie = name + '=' + escape(value) + ((numHours)?(';expires=' + numHours):'') + ((path)?';path=' + path:'') + ((domain)?';domain=' + domain:'') + ((secure && (secure == true))?';secure':''); // Set the cookie, adding any parameters that were specified.");document.writeln("    }");document.writeln("} // WM_setCookie");document.writeln("");document.writeln("function WM_readCookie(name) {");document.writeln("    if(document.cookie == '') { // there's no cookie, so go no further");document.writeln("	return false;");document.writeln("    } else { // there is a cookie");document.writeln("	var firstChar, lastChar;");document.writeln("	var theBigCookie = document.cookie;");document.writeln("	firstChar = theBigCookie.indexOf(name);	// find the start of 'name'");document.writeln("	var NN2Hack = firstChar + name.length;");document.writeln("	if((firstChar != -1) && (theBigCookie.charAt(NN2Hack) == '=')) { // if you found the cookie");document.writeln("	    firstChar += name.length + 1; // skip 'name' and '='");document.writeln("	    lastChar = theBigCookie.indexOf(';', firstChar); // Find the end of the value string (i.e. the next ';').");document.writeln("	    if(lastChar == -1) lastChar = theBigCookie.length;");document.writeln("	    return unescape(theBigCookie.substring(firstChar, lastChar));");document.writeln("	} else { // If there was no cookie of that name, return false.");document.writeln("	    return false;");document.writeln("	}");document.writeln("    }");document.writeln("} // WM_readCookie");document.writeln("");document.writeln("function WM_killCookie(name, path, domain) {");document.writeln("  var theValue = WM_readCookie(name); // We need the value to kill the cookie");document.writeln("  if(theValue) {");document.writeln("      document.cookie = name + '=' + theValue + '; expires=Fri, 13-Apr-1970 00:00:00 GMT' + ((path)?';path=' + path:'') + ((domain)?';domain=' + domain:''); // set an already-expired cookie");document.writeln("  }");document.writeln("} // WM_killCookie");document.writeln("// -->");document.writeln("</"+"SCRIPT>");document.writeln("");document.writeln("<script>");document.writeln("<!--");document.writeln("if (!(WM_readCookie('popunder'))) {");document.writeln("window.open('http:\\/\\/ads.ign.com/advertisers/phe/southpark/popunder_five.html','popunder','width=300,height=250,left=200,top=200').blur();");document.writeln("window.focus();");document.writeln("WM_setCookie('popunder','true','/','ign.com','false');");document.writeln("}");document.writeln("//-->");document.writeln("</"+"SCRIPT>");document.close();